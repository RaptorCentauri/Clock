  "bugs": {
    "url": ""
  },
  "homepage": "",
  "dependencies": {
    "@types/react": "^16.8.4",
    "axios": "^0.19.0",
    "codecov": "^3.7.1",
    "moment": "^2.24.0",
    "node-sass": "^4.9.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.1",
    "@types/jest": "^24.0.13",
    "enzyme": "^3.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-dom": "^3.4.0",
    "parcel-bundler": "^1.11.0",
    "react-addons-test-utils": "^15.6.2",
    "react-hooks-testing-library": "^0.5.0",
    "react-test-renderer": "^16.8.6",
    "sass": "^1.17.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.1"
  }
